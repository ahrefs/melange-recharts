{
  "name": "@ahrefs/bs-recharts",
  "version": "3.0.0",
  "description": "Reason bindings for recharts",
  "author": "ahrefs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ahrefs/bs-recharts.git"
  },
  "homepage": "https://github.com/ahrefs/bs-recharts#readme",
  "bugs": {
    "url": "https://github.com/ahrefs/bs-recharts/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "chart",
    "react-component",
    "bucklescript",
    "recharts",
    "binding"
  ],
  "scripts": {
    "prestart": "npm install && bsb -clean-world",
    "start": "npm run bs:watch",
    "test": "npm run bs:build",
    "bs:build": "bsb -make-world -clean-world",
    "bs:watch": "bsb -make-world -clean-world -w",
    "bs:setup": "bsb -make-world",
    "refmt": "find ./src -type f -name \"*.re\" | xargs bsrefmt --in-place"
  },
  "devDependencies": {
    "bs-platform": "^9.0.2",
    "reason-react": "^0.9.1"
  },
  "peerDependencies": {
    "bs-platform": "^9.0.2",
    "reason-react": "^0.9.1",
    "recharts": "^2.1.12"
  }
}
